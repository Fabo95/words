{
  "name": "eslint-plugin-words-app-styleguide",
  "version": "0.1.0",
  "main": "./lib/index.js",
  "scripts": {
    "initialize": "pnpm install && pnpm build:clean",
    "build": "tsc -d && tsc-alias",
    "build:clean": "rm -f ./tsconfig.tsbuildinfo && pnpm build",
    "test": "tsc --noEmit && jest",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:write": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@types/lodash": "4.17.13",
    "@types/node": "22.10.2",
    "@typescript-eslint/experimental-utils": "5.62.0",
    "@typescript-eslint/parser": "8.18.2",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-module-utils": "2.12.0",
    "eslint-plugin-import": "2.31.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "3.0.0",
    "@types/eslint": "9.6.1",
    "@types/jest": "29.5.14",
    "@typescript-eslint/types": "8.18.2",
    "eslint": "9.17.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.10",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "eslint": "9.17.0"
  }
}
