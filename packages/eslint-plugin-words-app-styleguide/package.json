{
  "name": "eslint-plugin-words-app-styleguide",
  "version": "0.1.0",
  "main": "./lib/index.js",
  "scripts": {
    "initialize": "pnpm install && pnpm build:clean",
    "build": "tsc -d && tsc-alias",
    "build:clean": "rm -f ./tsconfig.tsbuildinfo && pnpm build",
    "test": "tsc --noEmit && jest",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:write": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@types/lodash": "4.14.178",
    "@types/node": "18.0.1",
    "@typescript-eslint/experimental-utils": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-module-utils": "2.8.0",
    "eslint-plugin-import": "2.28.1",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "2.2.1",
    "@types/eslint": "8.4.6",
    "@types/jest": "27.4.0",
    "@typescript-eslint/types": "5.9.1",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "prettier": "2.7.1",
    "ts-node": "10.7.0",
    "tsc-alias": "1.6.4",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "eslint": "8.57.0"
  }
}
